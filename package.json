{
  "name": "boil-cli-tool",
  "description": "CLI tool for generating boilerplate files and folders",
  "version": "0.0.0",
  "author": "Jordan-Eckowitz @Jordan-Eckowitz",
  "bin": {
    "boil": "./bin/run"
  },
  "bugs": "https://github.com/Jordan-Eckowitz/boil-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.165",
    "@types/node-emoji": "^1.8.1",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "node-emoji": "^1.10.0",
    "read-data": "^1.1.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.25.1",
    "@types/node": "^10.17.48",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Jordan-Eckowitz/boil-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "boil",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Jordan-Eckowitz/boil-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
